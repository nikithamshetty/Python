print("The Love Calculator is calculating your score...")
name1 = input() # What is your name?
name2 = input() # What is their name?
# ðŸš¨ Don't change the code above ðŸ‘†
# Write your code below this line ðŸ‘‡
combined_names=name1+name2
lower_letters=combined_names.lower()
t= lower_letters.count("t")
r= lower_letters.count("r")
u= lower_letters.count("u")
e= lower_letters.count("e")
first_digit= t + r + u + e

l= lower_letters.count("l")
o= lower_letters.count("o")
v= lower_letters.count("v")
e= lower_letters.count("e")
second_digit= l + o + v + e

combined=int(str(first_digit)+str(second_digit))
if (combined < 10) or (combined > 90):
  print(f"Your score is {combined}, you go together like coke and mentos.")
elif (combined > 40) and (combined < 50):
  print(f"Your score is {combined}, you are alright together.")
else:
  print(f"Your score is {combined}.")
